{
  "compilerOptions": {
    "target": "ES2018",
    "module": "es6",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": false,
    "noImplicitAny": false,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    },
    "types": [
      "@nuxt/types",
      "@types/node",
      "@types/jest"
    ],
    "jsx": "preserve",
    "jsxFactory": "VueTsxSupport"
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
